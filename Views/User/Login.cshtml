


@{
    ViewBag.Title = "Login";
}

<form asp-action="Login" method="post">
    <div>
        <label>User Login ID:</label>
        <input name="userloginid" required />
    </div>
    <div>
        <label>Password:</label>
        <input name="userpass" type="password" required />
    </div>
    <button type="submit">Login</button>
    <div>@ViewBag.Msg</div>
</form>
